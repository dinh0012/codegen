{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./tsc",
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "allowJs": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules"
  ]
}
